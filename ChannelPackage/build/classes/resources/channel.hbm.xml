<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
          "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bean.channel" table="channelDetails">
		<id name="cid" column="ID">
			<generator class="increment"></generator>
		</id> 
		<property name="chargeType" column="ChargingType"></property>
		<property name="transType" column="TransmissionType"></property>
		<property name="chan" column="Channel"></property>
	</class>
</hibernate-mapping>
